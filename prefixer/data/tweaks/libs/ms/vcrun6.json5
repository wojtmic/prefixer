{
  description: "MS Visual C++ 6.0 Runtime",
  tasks: [
    {
      description: "Set DLL overrides for VC6 runtime",
      type: "regedit",
      path: "Software\\Wine\\DllOverrides",
      filename: 'user.reg',
      values: {
        mfc42: "native,builtin",
