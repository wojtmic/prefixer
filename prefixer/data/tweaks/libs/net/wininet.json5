{
  description: "Native wininet (fixes login timeouts in launchers)",
  tasks: [
    {
      description: "Set wininet DLL override",
      type: "regedit",
      path: "Software\\Wine\\DllOverrides",
      filename: 'user.reg',
      values: {
        wininet: "native,builtin"
      }
    }
  ]
}
