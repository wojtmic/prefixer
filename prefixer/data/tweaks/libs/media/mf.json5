{
  description: "Media Foundation (fixes video in modern games like RE/MH)",
  tasks: [
    {
      description: "Set Media Foundation DLL overrides",
      type: "regedit",
      path: "Software\\Wine\\DllOverrides",
      filename: 'user.reg',
      values: {
        mf: "native,builtin",
        mfplat: "native,builtin",
        mfreadwrite: "native,builtin",
        mfmkvsource: "native,builtin",
        mfplay: "native,builtin"
      }
    },
    {
      description: "Information about MF",
      type: "message",
      content: "Media Foundation overrides set. For full functionality, ensure wine-staging or Proton-GE is being used as they include MF patches."
    }
  ]
}
