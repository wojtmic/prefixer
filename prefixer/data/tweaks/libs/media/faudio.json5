{
  description: "FAudio/XAudio2 (from FNA native libs)",
  tasks: [
    {
      description: "Download FNA native libs (includes FAudio)",
      type: "download",
      url: "https://fna.flibitijibibo.com/archive/FNA-2512.zip",
      checksum: "db2dfd63f0c3de7f1632f781ba757410c1035eabe0311c7e13e5a0bec622cfb1",
      filename: "fna.zip"
    },
    {
      description: "Extract FNA",
      type: "extract",
      filename: "fna.zip",
      path: "<tempdir>/fna"
    },
    {
      description: "Copy FAudio x64 DLL",
      type: "copy",
      path: "<tempdir>/fna/fnalibs/x64/FAudio.dll",
      new_path: "<pfxdir>/drive_c/windows/system32/FAudio.dll"
    },
    {
      description: "Copy FAudio x86 DLL",
      type: "copy",
      path: "<tempdir>/fna/fnalibs/x86/FAudio.dll",
      new_path: "<pfxdir>/drive_c/windows/syswow64/FAudio.dll"
    },
    {
      description: "Set DLL overrides for XAudio",
      type: "regedit",
      path: "HKEY_CURRENT_USER\\Software\\Wine\\DllOverrides",
      values: {
        xaudio2_0: "native,builtin",
        xaudio2_1: "native,builtin",
        xaudio2_2: "native,builtin",
        xaudio2_3: "native,builtin",
        xaudio2_4: "native,builtin",
        xaudio2_5: "native,builtin",
        xaudio2_6: "native,builtin",
        xaudio2_7: "native,builtin",
        xaudio2_8: "native,builtin",
        xaudio2_9: "native,builtin"
      }
    }
  ]
}
