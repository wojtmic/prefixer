{
  description: "ActiveMovie Multimedia Streaming (legacy video)",
  tasks: [
    {
      description: "Set DLL override for amstream",
      type: "regedit",
      path: "Software\\Wine\\DllOverrides",
      filename: 'user.reg',
      values: {
        amstream: "native,builtin"
      }
    },
    {
      description: "Register amstream.dll",
      type: "run_exe",
      path: "regsvr32",
      args: ["/s", "amstream.dll"]
    }
  ]
}
