{
  description: "Windows Media Player 10 - DLL Overrides",
  tasks: [
    {
      description: "Set WMP DLL overrides",
      type: "regedit",
      path: "Software\\Wine\\DllOverrides",
      filename: 'user.reg',
      values: {
        wmvcore: "native,builtin",
        wmp: "native,builtin",
        wmpdxm: "native,builtin",
        wmpeffects: "native,builtin"
      }
    }
  ]
}
