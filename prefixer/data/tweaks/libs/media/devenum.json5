{
  description: "Device Enumerator (DirectShow audio/video detection)",
  tasks: [
    {
      description: "Set DLL override for devenum",
      type: "regedit",
      path: "Software\\Wine\\DllOverrides",
      filename: 'user.reg',
      values: {
        devenum: "native,builtin"
      }
    },
    {
      description: "Register devenum.dll",
      type: "run_exe",
      path: "regsvr32",
      args: ["/s", "devenum.dll"]
    }
  ]
}
