{
  description: "DirectShow Quartz filter (video playback)",
  tasks: [
    {
      description: "Set DLL override for quartz",
      type: "regedit",
      path: "Software\\Wine\\DllOverrides",
      filename: 'user.reg',
      values: {
        quartz: "native,builtin"
      }
    },
    {
      description: "Register quartz.dll",
      type: "run_exe",
      path: "regsvr32",
      args: ["/s", "quartz.dll"]
    }
  ]
}
