{
  description: "Virtual Desktop 1280x720",
  tasks: [
    {
      description: "Enable virtual desktop",
      type: "regedit",
      path: "Software\\Wine\\Explorer\\Desktops",
      filename: 'user.reg',
      values: {
        Default: "1280x720"
      }
    },
    {
      description: "Set explorer to use virtual desktop",
      type: "regedit",
      path: "Software\\Wine\\Explorer",
      filename: 'user.reg',
      values: {
        Desktop: "Default"
      }
    }
  ]
}
